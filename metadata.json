{
  "name": "puppet-jail",
  "version": "4.0.1",
  "author": "Vox Pupli",
  "summary": "Jail management with Puppet",
  "license": "Apache-2.0",
  "source": "git://github.com/voxpupuli/puppet-jail.git",
  "project_page": "https://github.com/voxpupuli/puppet-jail",
  "issues_url": "https://github.com/voxpupuli/puppet-jail/issues",
  "tags": [
    "FreeBSD",
    "jails",
    "containers",
    "iocage",
    "zfs"
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "10",
        "11",
        "12",
        "13"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.1.0 < 8.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.1.0 <= 5.0.0"
    }
  ]
}
